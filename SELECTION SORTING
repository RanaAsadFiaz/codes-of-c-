#include <iostream>
using namespace std;
int main()
{
	int arr[8];
	int temp;
	for (int i=0;i<8;i++)
	{
		cout<<"ENTER THE VALUE:"<<endl;
		cin>>arr[i];
	}
	for (int j=0;j<7;j++)
	{
		for(int k=j;k<8;k++)
		{
			if(arr[j]>arr[k])
			{
			temp=arr[j];
			arr[j]=arr[k];
			arr[k]=temp;
		}
	}
	}
	cout<<"the sorted values are :"<<endl;
	for(int l=0;l<8;l++)
	{
		cout<<arr[l]<<"  ";
	}
}
